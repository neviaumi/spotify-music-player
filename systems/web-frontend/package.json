  "scripts": {
    "build": "snowpack build",
    "lint:css": "stylelint --fix './src/**/*.tsx'",
    "lint:css:ci": "stylelint './src/**/*.tsx'",
    "lint:script": "eslint --ext=ts,tsx,json,js --fix .",
    "lint:script:ci": "eslint --ext=ts,tsx,json,js .",
    "postinstall": "patch-package",
    "serve:lhci": "serve -l 3000 -s build",
    "signToken": "source ./.env && npx cli sign-token \"$SNOWPACK_PUBLIC_SPOTIFY_CLIENT_ID\" \"$SNOWPACK_PUBLIC_SPOTIFY_REQUESTED_SCOPE\" | npx cli set-env-file \"$PWD/.env.test\" SNOWPACK_PUBLIC_SPOTIFY_ACCESS_TOKEN",
    "start": "snowpack dev",
    "test": "start-server-and-test 'polly listen -p 3001' 3001 'npm run wtr'",
    "test:ci": "start-server-and-test 'polly listen -p 3001' 3001 'CI=true npm run wtr -- --concurrent-browsers 1 --concurrency 1'",
    "test:reset": "rimraf ./recordings/** && npm run signToken && start-server-and-test 'polly listen -p 3001' 3001 'npm run wtr:record'",
    "wtr": "web-test-runner",
    "wtr:record": "SNOWPACK_PUBLIC_POLLY_MODE=record web-test-runner --concurrent-browsers 1 --concurrency 1"
  },
  "version": "0.1.0"
}
