    "build": "react-scripts build",
    "dev": "react-scripts start",
    "eject": "react-scripts eject",
    "lint:css": "stylelint --fix './src/**/*.{ts,tsx}'",
    "lint:css:ci": "stylelint './src/**/*.{ts,tsx}'",
    "lint:package-json": "npmPkgJsonLint .",
    "lint:script": "eslint --ext=ts,tsx,json,js --fix .",
    "lint:script:ci": "eslint --ext=ts,tsx,json,js .",
    "postinstall": "husky install",
    "serve:lhci": "serve -l 42069 -s build",
    "signToken": "node ./testHelper/signToken/index.js",
    "start": "react-scripts start",
    "test": "react-scripts test --maxWorkers=50% --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --runInBand --coverage",
    "test:reset": "rimraf ./src/**/*.har && npm run signToken && REACT_APP_POLLY_MODE=record react-scripts test --watchAll=false --runInBand --testTimeout 60000"
  },
