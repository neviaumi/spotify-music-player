  "engines": {
    "node": ">=14"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "clover",
      "json"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 64,
        "functions": 84,
        "lines": 80,
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(parse-ms)).*\\.js$"
    ]
  },
  "license": "MIT",
  "name": "@music-player/spotify",
  "private": true,
  "scripts": {
    "build": "yarn build:functions && yarn build:react",
    "build:functions": "netlify-lambda build functions",
    "build:react": "react-scripts build",
    "dev": "netlify dev",
    "eject": "react-scripts eject",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:package-json": "yarn npmPkgJsonLint .",
    "lint:script": "eslint --ext=ts,tsx,json,js --fix .",
    "lint:script:ci": "eslint --ext=ts,tsx,json,js .",
    "postinstall": "husky install && netlify-lambda install",
    "quicktype": "quicktype --just-types",
    "serve:lhci": "serve -l 42069 -s build",
    "signToken": "node ./testHelper/signToken/index.js",
    "start": "react-scripts start",
    "test": "yarn test:react && yarn test:functions",
    "test:ci": "yarn test:ci:react && yarn test:ci:functions",
    "test:ci:functions": "CI=true jest --config jest.config.functions.js --runInBand --coverage",
    "test:ci:react": "CI=true react-scripts test --runInBand --coverage",
    "test:functions": "jest --config jest.config.functions.js",
    "test:functions:reset": "rimraf ./functions/**/*.har && REACT_APP_POLLY_MODE=record jest --config jest.config.functions.js --watchAll=false --runInBand --testTimeout 60000",
    "test:react": "react-scripts test --maxWorkers=50% --coverage --watchAll=false",
    "test:react:reset": "rimraf ./src/**/*.har && yarn signToken && REACT_APP_POLLY_MODE=record react-scripts test --watchAll=false --runInBand --testTimeout 60000",
    "test:reset": "yarn test:react:reset && yarn test:functions:reset"
  },
  "version": "0.1.0"
}
